<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
	        http://code.alibabatech.com/schema/dubbo
	        http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

	<dubbo:registry id="baseServiceRegistry" address="${dubbo.baseservice.registry.address}" client="zkclient" check="false"/>
	<dubbo:reference id="idGenerationFacade"  interface="com.touna.base.service.facade.intf.IdGenerationFacade" registry="baseServiceRegistry" version="${dubbo.baseservice.version}"/>
</beans>